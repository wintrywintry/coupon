/* pages/familyCard/familyCard.wxss */

.header {
  display: flex;
  align-items: center;
  height: 320rpx;
  padding: 0 40rpx;
  font-size: 32rpx;
  color: #acacac;
  text-align: center;
  justify-content: space-between;
}

.minimg {
  width: 110rpx;
  height: 110rpx;
  margin-bottom: 20rpx;
}

.bigtxt {
  color: #7c7c7c;
}

.bigimg {
  width: 210rpx;
  height: 210rpx;
  margin-bottom: 20rpx;
}

.banner {
  text-align: center;
}

.banerimg {
  width: 700rpx;
  height: 400rpx;
}

.titlecardtxt {
  color: #545045;
}

.titlecard {
  padding: 0 40rpx;
  margin: 40rpx 0 30rpx 0;
}

.yunimg {
  font-size: 34rpx;
  width: 80rpx;
  height: 46rpx;
  margin-right: 20rpx;
}
.cards{
  display: flex;
  align-content: center;
  justify-content: space-between;
  padding: 0 40rpx;
}
.five{
  width: 320rpx;
  font-size: 25rpx;
  line-height: 40rpx;
  border-radius:  20rpx;
}
.imglist {
  width: 320rpx;
  height: 200rpx;
}
.fivetxt{
  display: flex;
  justify-content: space-between;
}
.fivemoney{
  color: #d83f42;
}
.throunthline{
  color: #7c7c7c;
  font-size: 20rpx;
  text-decoration: line-through;
}
.news{
  color: #7c7c7c;
  text-align: center;
  margin-bottom: 10rpx;
}
.linerstyle{
  height: 20rpx;
  width: 310rpx;
  display: flex;
  margin-top: 10rpx;
  align-items: center;
  background: linear-gradient(to bottom, #f0f0f0, white);
}
