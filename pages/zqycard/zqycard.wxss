/* pages/zqycard/zqycard.wxss */
.title{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  height: 200rpx;
   width: 100%;
   margin: 20rpx 0;
}
.title>view{
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    width: 140rpx; 
    align-items: center;
}
.title>view>text{
  color: gray;
  font-size: 25rpx;
}
.title>view>image{
  width: 100rpx;
  height: 100rpx;
}
.img{
   width: 130rpx;
   height:130rpx;  
}
.middlecontent{
   height: 500rpx;
   width: 100%;
   display: flex;
   justify-content: center;
}
.middle_img{
   height: 500rpx;
   width: 95%;
   margin: 0 auto;
}
  .card{
    height: 50rpx;
    width: 100%;
      display: flex;
      justify-content: flex-start;
      margin: 20rpx 0;
      font-weight: bolder;
  }
.card>image{
      width: 100rpx;
      height: 50rpx;
      margin:0  20rpx;
    }
.togter{
   width: 100%;
   height: 340rpx;
   display: flex;
   justify-content: space-between;
}
.togter>image{
   width: 350rpx;
   height: 300rpx;
   box-shadow:0rpx 20rpx 10rpx #d6d2d2; 
}
.togter>image:first-child{
   margin-left: 10rpx;
}
.togter>image:last-child{
   margin-right: 10rpx;
}
.buttom{
  width: 100%;
  height: 400rpx;
  display: flex;
  justify-content: flex-start;
}
.buttom>image{
  width: 350rpx;
  height: 300rpx;
  margin-left: 20rpx;
   box-shadow:0rpx 20rpx 10rpx #d6d2d2; 
}

